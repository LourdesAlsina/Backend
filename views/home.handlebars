<div>
    <h1>Que nos ponemos hoy?</h1>
        <div class="container">
            <h2>Que nos ponemos hoy?</h2>
            <table>
                <thead>
                    <tr>
                        <th>Title</th>
                        <th>Description</th>
                        <th>Price</th>
                        <th>Code</th>
                        <th>Category</th>
                        <th>Stock</th>
                    </tr>
                </thead>
                {{#each allProducts}}
                    <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.code}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.stock}}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
</div>      
                

